    "standard-reporter": "^1.0.5",
    "storybook-react-router": "^1.0.3",
    "stylefmt": "^6.0.3",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "svelte": "^3.46.3",
    "svelte-adapter": "^0.3.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "2.12.0",
    "webpack": "4.44.2",
    "webpack-cli": "^3.1.2",
    "why-did-you-update": "^0.1.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/serviceWorker.js",
      "build"
    ]
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "yarn format",
        "git add"
      ],
      "*.{sass,scss,css}": [
        "yarn stylefmt",
        "yarn stylelint src/*{sass,scss} --syntax scss --fix",
        "git add"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
