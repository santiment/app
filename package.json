    "babel-loader": "^8.0.5",
    "circular-dependency-plugin": "^5.2.0",
    "copy-webpack-plugin": "5.1.2",
    "core-js": "2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.2.2",
    "exit_on_eof": "^1.0.4",
    "imports-loader": "^0.7.1",
    "lint-staged": "^7.3.0",
    "node-sass": "npm:sass@^1.43.4",
    "postcss": "8.2.13",
    "postcss-cssnext-3": "npm:postcss-cssnext@3",
